{
  "name": "metaexodus",
  "version": "1.0.0",
  "description": "API-powered migration from Metabase to local Postgres",
  "type": "module",
  "main": "sync-all.js",
  "scripts": {
    "start": "node sync-all.js",
    "sync": "node sync-all.js",
    "sync-safe": "node scripts/sync-with-exclusions.js",
    "sync-partial": "node scripts/sync-partial.js",
    "create-schema": "node scripts/create-schema.js",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest",
    "test:watch": "NODE_OPTIONS='--experimental-vm-modules' jest --watch",
    "test:coverage": "NODE_OPTIONS='--experimental-vm-modules' jest --coverage",
    "test:special-chars": "node examples/test-special-chars.js",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write ."
  },
  "packageManager": "yarn@1.22.0",
  "keywords": [
    "database",
    "sync",
    "postgresql",
    "migration"
  ],
  "author": "Salih Muhammed <lr0@gmx.com>",
  "license": "GPL-3.0-only",
  "dependencies": {
    "axios": "^1.10.0",
    "chalk": "4.1.2",
    "cli-progress": "^3.12.0",
    "dotenv": "^16.3.1",
    "ora": "5.4.1",
    "pg": "^8.11.3",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.4",
    "jest": "29.7.0",
    "nodemon": "^3.0.1",
    "string-width": "4.2.3",
    "strip-ansi": "6.0.1",
    "supports-color": "^10.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
